{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "keyofStringsOnly": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "paths": {
      "Components/*": [
        "Components/*"
      ],
      "Containers/*": [
        "Containers/*"
      ],
      "Mocks/*": [
        "Mocks/*"
      ],
      "Services/*": [
        "Services/*"
      ],
      "Shared/*": [
        "Shared/*"
      ],
      "Types/*": [
        "Types/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "webpack.config.js",
    "webpack.config.js",
    "webpack.prod.js",
  ]
}